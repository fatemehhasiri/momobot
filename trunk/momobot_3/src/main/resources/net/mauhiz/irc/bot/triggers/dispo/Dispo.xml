<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="net.mauhiz.irc.bot.triggers.dispo.Dispo" table="DISPO">
        <id column="DISPO_ID" name="id" type="java.lang.Long">
            <generator class="native" />
        </id>
        <property column="DISPO_WHEN" name="when" type="java.sql.Date" />
        <property column="DISPO_CHANNEL" name="channel" type="java.lang.String" />
        <property column="DISPO_SERVER" name="serverAlias" type="java.lang.String" />
        <property column="DISPO_QAUTH" name="qauth" type="java.lang.String" />
    </class>
</hibernate-mapping>